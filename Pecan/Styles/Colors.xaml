<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!--Primary Colors-->
    <Color x:Key="oliveDrab">#689A40</Color>
    <Color x:Key="oliveDrabL1">#87B95A</Color>
    <Color x:Key="oliveDrabD1">#51832D</Color>
    <Color x:Key="oliveDrabD2">#3A6C19</Color>
    
    <!--Secondary Colors-->
    <Color x:Key="tomThumb">#435739</Color>
    <Color x:Key="tomThumbL1">#536848</Color>
    <Color x:Key="tomThumbD1">#374B2E</Color>
    <Color x:Key="tomThumbD2">#2B3E24</Color>
    
    <!--Color Palette-->
    <SolidColorBrush x:Key="oliveBrush" Color="#689A40" />
    <SolidColorBrush x:Key="lightOliveBrush" Color="#87B95A" />
    <SolidColorBrush x:Key="darkOliveBrush" Color="#3A6C19" />
    <SolidColorBrush x:Key="tomBrush" Color="#435739" />
    <SolidColorBrush x:Key="lightTomBrush" Color="#536848" />
    <SolidColorBrush x:Key="darkTomBrush" Color="#2B3E24" />

</ResourceDictionary>